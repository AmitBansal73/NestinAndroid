<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" tools:context="net.anvisys.NestIn.MyProfile"
    style="@style/Theme.AppCompat.Light.NoActionBar">

    <LinearLayout
    android:layout_width="350dp"
    android:layout_height="500dp"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="10dp"
    android:orientation="vertical"
    android:background="@drawable/box_border"
    >
    <!-- TODO: Update blank fragment layout -->
    <TextView
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:text="FirstName"
    android:layout_marginTop="20dp"
    android:layout_marginLeft="10dp"/>
    <EditText
    android:id="@+id/editFirstName"
    android:layout_width="300dp"
    android:layout_height="30dp"
    android:layout_marginTop="1dp"
    android:layout_marginLeft="10dp"
    android:hint="First Name"
    android:background="@drawable/box_border"
    android:editable="false"
    />

    <TextView
    android:layout_width="wrap_content"
    android:layout_height="20dp"
    android:text="MiddleName"
    android:layout_marginTop="10dp"
    android:layout_marginLeft="10dp"/>
    <EditText
    android:id="@+id/editMiddleName"
    android:layout_width="300dp"
    android:layout_height="30dp"
    android:layout_marginTop="1dp"
    android:layout_marginLeft="10dp"
    android:hint="Middle Name"
    android:background="@drawable/box_border"
    android:editable="false"
        />

        <TextView
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text="LastName"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"/>
        <EditText
        android:id="@+id/editLastName"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="10dp"
        android:hint="Last Name"
        android:background="@drawable/box_border"
        android:editable="false"
            />
        <TextView
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:text="SocietyName"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"/>

        <EditText
        android:id="@+id/editSocietyName"
        android:layout_width="300dp"
        android:layout_height="30dp"
        android:layout_marginTop="1dp"
        android:layout_marginLeft="10dp"
        android:hint="Society"
        android:background="@drawable/box_border"
        android:editable="false"
        />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="UserLogin"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"/>
        <EditText
            android:id="@+id/editUserLogin"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="10dp"
            android:hint="Login Name"
            android:background="@drawable/box_border"
            android:editable="false"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="MobileNo"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"/>

        <EditText
            android:id="@+id/editMobileNo"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="10dp"
            android:hint="Mobile"
            android:background="@drawable/box_border"
            android:editable="false"
            />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="20dp"
            android:text="EMail"
            android:layout_marginTop="10dp"
            android:layout_marginLeft="10dp"/>
        <EditText
            android:id="@+id/editEmail"
            android:layout_width="300dp"
            android:layout_height="30dp"
            android:layout_marginTop="1dp"
            android:layout_marginLeft="10dp"
            android:hint="Email"
            android:background="@drawable/box_border"
            android:editable="false"
            />
    </LinearLayout>
</FrameLayout>
